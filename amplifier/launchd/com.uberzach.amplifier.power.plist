		<key>OtherJobEnabled</key>
		<dict>
			<key>com.uberzach.amplifier</key>
			<true/>
		</dict>
	</dict>
	<key>ProgramArguments</key>
	<array>
		<string>/Users/tv/bin/video/amplifier/amplifierPower.pl</string>
	</array>
</dict>
</plist>
